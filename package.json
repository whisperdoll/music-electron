{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "build": "tsc",
    "watchtsc": "tsc -w",
    "watchsass": "sass --watch style/style.scss style/style.css",
    "watch": "npm-run-all watchtsc watchsass",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "electron ./dist/main.js"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/archiver": "^2.1.3",
    "@types/chokidar": "^1.7.5",
    "archiver": "^3.0.0",
    "chokidar": "^2.1.6",
    "clubber": "^1.7.1",
    "electron": "18.3.7",
    "music-metadata": "^3.8.0",
    "node-dir": "^0.1.17",
    "npm-run-all": "^4.1.5",
    "sass": "^1.20.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "wavesurfer.js": "https://github.com/katspaugh/wavesurfer.js.git"
  },
  "dependencies": {
    "@types/node": "^10.14.7"
  }
}
